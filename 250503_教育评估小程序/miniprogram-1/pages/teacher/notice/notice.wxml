<view class="container">
  <view class="page-header">
    <text>校园通知</text>
  </view>
  <view class="notice-list">
    <block wx:for="{{notices}}" wx:key="index">
      <view class="notice-item" bindtap="goToNoticeDetail" data-path="{{item.detailPath}}">
        <text>{{item.title}}</text>
        <text style="float: right;">{{item.date}}</text>
      </view>
    </block>
  </view>
</view>