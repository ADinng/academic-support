<view class="container">
    <!-- 顶部课程图片 -->
    <image src="{{detailImage}}" class="detail-image" mode="widthFix"></image>

    <!-- 跳转链接区域（按需保留按钮） -->
    <view class="copy-container">
        <button bindtap="navigateToOtherPage" class="copy-button">点击跳转</button>
    </view>

    <!-- 课程描述区域 -->
    <view class="description">
        <text>{{description}}</text>
    </view>
</view>    